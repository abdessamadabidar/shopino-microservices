#!user/bin/env groovy
@Library('jenkins-shared-library')_

pipeline {
    agent any
    tools {
        maven "maven"
    }

    stages {

        stage("build Jar") {
            steps {
                script {
                    buildJar 'config-service'
                }
            }
        }

        stage("build and push Image") {
            steps {
                script {
                    dockerLogin()
                }
            }
        }

    }
}